{
    "name": "mugenzo/laravel-shopping-cart",
    "description": "Shopping cart for laravel",
    "type": "library",
    "authors": [
        {
            "name": "Mugenzo",
            "email": "mugenzo@outlook.com"
        }
    ],
    "require": {
        "illuminate/support": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0",
        "illuminate/session": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0",
        "illuminate/events": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0",
        "nesbot/carbon": "~1.20||^1.26.3||^2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0||~6.0||~7.0||~8.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Mugenzo\\LaravelShoppingCart\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mugenzo\\LaravelShoppingCart\\ShoppingCartServiceProvider"
            ],
            "aliases": {
                "Cart": "Mugenzo\\LaravelShoppingCart\\Facades\\Cart"
            }
        }
    }
}
